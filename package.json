{
  "name": "@viaprotocol/router-sdk",
  "version": "1.0.3",
  "dependencies": {
    "axios": "^0.26.1"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "tsc --build --clean",
    "watch": "tsc -w -p ./tsconfig.json",
